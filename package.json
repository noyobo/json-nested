{
  "name": "json-nested",
  "version": "2.1.1",
  "description": "Parse all possible nested JSON strings.",
  "keywords": [
    "json-parse"
  ],
  "homepage": "https://github.com/noyobo/json-nested#readme",
  "bugs": {
    "url": "https://github.com/noyobo/json-nested/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noyobo/json-nested.git"
  },
  "license": "MIT",
  "author": {
    "name": "Youbao Nong",
    "email": "noyobo@gmail.com",
    "url": "https://github.com/noyobo"
  },
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "files": [
    "lib/",
    "esm/",
    "types/"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:lib",
    "build:esm": "swc src -d esm --strip-leading-paths -C module.type=es6 -C jsc.target=es2017 -C minify=true -C jsc.minify.mangle=true",
    "build:lib": "swc src -d lib --strip-leading-paths  -C module.type=commonjs -C minify=true -C jsc.minify.mangle=true",
    "clean": "rm -rf ./lib ./esm types",
    "lint": "eslint ./src --fix",
    "prepare": "npx husky install",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "dependencies": {},
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "vitest": "^2.0.1"
  },
  "engines": {
    "node": ">=12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
