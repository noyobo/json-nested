{
  "name": "json-all",
  "version": "2.1.1",
  "description": "Parse all possible nested JSON strings.",
  "keywords": [
    "json-parse"
  ],
  "homepage": "https://github.com/noyobo/json-all#readme",
  "bugs": {
    "url": "https://github.com/noyobo/json-all/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noyobo/json-all.git"
  },
  "license": "MIT",
  "author": {
    "name": "Youbao Nong",
    "email": "noyobo@gmail.com",
    "url": "https://github.com/noyobo"
  },
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:esm": " swc src -d esm --strip-leading-paths -C module.type=es6 -C jsc.target=es2017 -C minify=true -C jsc.minify.mangle=true",
    "build:lib": " swc src -d lib --strip-leading-paths  -C module.type=commonjs -C minify=true -C jsc.minify.mangle=true",
    "clean": "rm -rf ./lib ./esm types",
    "lint": "eslint ./src --fix",
    "prepare": "npx husky install"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "husky": "^9.0.11"
  },
  "engines": {
    "node": ">=12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
